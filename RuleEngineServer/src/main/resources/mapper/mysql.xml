<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.tongdun.rule">
    <select id="getRule" parameterType="String" resultType="String">
        select gremlin from rule
        where rule_id=#{ruleId}
    </select>
</mapper>